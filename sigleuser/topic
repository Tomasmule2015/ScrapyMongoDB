<script src="http://x.autoimg.cn/club/v1Content/js/jquery.js" type="text/javascript"></script>
<script type="text/javascript">
    var userTopic = userTopic || {};
    userTopic.bigBadgeShow = function (bigurl, t, c,url) {
        $("#badgetitle").html(t);
        $("#badgecontent").html(c);
        $("#bigbadgeimg").attr("src", bigurl);
        $("#medalInfo").show();
        $("#badgetitle").attr("href", url);
        if (url == "#") { $("#badgetitle").removeAttr("target"); }
        else { $("#badgetitle").attr("target", "_blank");}
    }
    userTopic.bigBadgeHide = function () {
        $("#medalInfo").hide();
    }
    $("#medalInfo").hover(function () { $(this).show(); }, function () { $(this).hide(); });
    $("#badgetitle").hover(function () { $(this).attr({ "style": "color:red" }); }, function () { $(this).removeAttr("style"); });

        userTopic.medalState = function () {
            var medalList = $("#medalList");
            var medalA = $("#medalA");
            var medalItems = $("#medalList a");
            var showMore = false;
            if (medalList.attr("class") == "floatLeft medal-img") {
            	showMore = true;
                medalList.addClass("medalSlide");
                medalA.html("收起<em class='ico_arrows'></em>").removeClass("meal_all").addClass("meal_dem");
            }
            else {
            	showMore = false;
                medalList.removeClass("medalSlide");
                medalA.html("展开全部勋章<em class='ico_arrows'></em>").removeClass("meal_dem").addClass("meal_all");
            }
            for (var i = 12; i < medalItems.length; i++) {
            	if (showMore) {
            		medalItems.eq(i).show();
            	} else {
            		medalItems.eq(i).hide();
            	}
            }
        }

        var d = document,
            mode = d.documentElement ? "documentElement" : "body";
        var EventUtil = {
            $el: function (id) {
                return typeof id === "string" ? document.getElementById(id) : id;
            },
            addEvent: function (el, type, fn) {
                if (el.addEventListener) {
                    el.addEventListener(type, fn, false);
                } else if (el.attachEvent) {
                    el.attachEvent("on" + type, fn);
                } else {
                    el["on" + type] = fn;
                }
            },
            removeEvent: function (el, type, fn) {
                if (el.removeEventListener) {
                    el.removeEventListener(type, fn, false);
                } else if (el.detachEvent) {
                    el.detachEvent("on" + type, fn);
                } else {
                    el["on" + type] = null;
                }
            },
            browser: {
                isIE: window.attachEvent && !window.opera,
                isIE6: navigator.userAgent.indexOf("MSIE 6") != -1 && !window.opera,
                chrome: navigator.userAgent.toLowerCase().indexOf("chrome") != -1
            },
            getViewPort: function () {
                var w = Math.max(d[mode].clientWidth, d[mode].scrollWidth);
                var h = Math.max(d[mode].clientHeight, d[mode].scrollHeight);
                return {
                    width: w,
                    height: h
                };
            },
            getOffset: function (target) {
                var left = target.offsetLeft,
                    top = target.offsetTop,
                    parent = target.offsetParent;
                while (parent != null) {
                    left += parent.offsetLeft;
                    top += parent.offsetTop;
                    parent = parent.offsetParent;
                }
                return {
                    oLeft: left,
                    oTop: top
                };
            },
            getEvent: function (e) {
                return e ? event : window.event;
            },
            prevent: function (e) {
                if (e.preventDefault) {
                    e.preventDefault();
                } else {
                    e.returnValue = false;
                }
            }
        };
        function medal() {
            var medalList = EventUtil.$el("medalList").getElementsByTagName("img");
            var t;
            for (var i = 0, len = medalList.length; i < len; ++i) {
                medalList[i].onmouseover = function (event) {
                    var flag = false;
                    event = event || window.event;
                    var ev = {
                        target: event.target || event.srcElement,
                        offsetX: event.offsetX || (event.clientX - EventUtil.getOffset(this).oLeft),
                        offsetY: event.offsetY || (event.clientY - EventUtil.getOffset(this).oTop),
                        clientX: event.clientX, clientY: event.clientY
                    };
                    var _self = this;
                    var ox = ev.offsetX,
                        oy = ev.offsetY;
                    ox = ox < 0 ? 0 : ox;
                    oy = oy < 0 ? 0 : oy;
                    var x = ev.clientX - ox,
                            y = ev.clientY - oy;
                    if (EventUtil.browser.isIE6) {
                        //x = ev.clientX - ox + d["documentElement"].scrollLeft + d["body"].scrollLeft - 375
                        //y = ev.clientY - oy + d["documentElement"].scrollTop + d["body"].scrollTop - 5;
                        x = ev.clientX - ox + d["documentElement"].scrollLeft + d["body"].scrollLeft
                        y = ev.clientY - oy + d["documentElement"].scrollTop + d["body"].scrollTop;
                    }
                    else if (EventUtil.browser.isIE) {
                        x = ev.clientX - ox + d["documentElement"].scrollLeft + d["body"].scrollLeft;
                        y = ev.clientY - oy + d["documentElement"].scrollTop + d["body"].scrollTop;
                    }

                    if (x + 297 > $("body").width()) {
                        $("#medalInfo .arrow2").addClass("arrow2_right");
                        x = x - 257;
                    } else {
                        $("#medalInfo .arrow2").removeClass("arrow2_right");
                    }

                    EventUtil.$el("medalInfo").style.display = "block";
                    EventUtil.$el("medalInfo").style.left = x - 10 + "px";
                    EventUtil.$el("medalInfo").style.top = y - 17 + "px";
                    var sh = function (e) {
                        if (t) clearTimeout(t);
                        e = e || window.event;
                        var el = e.target || e.srcElement;
                        while (el) {
                            flag = el == _self || el == EventUtil.$el("medalInfo");
                            if (flag) break;
                            el = el.parentNode;
                        }
                        if (!flag) {
                            t = setTimeout(function () {
                                EventUtil.$el("medalInfo").style.display = "none";
                            }, 500);
                            EventUtil.removeEvent(document, "mouseover", sh);
                        }
                    };
                    EventUtil.addEvent(document, "mouseover", sh);
                };
            }
        }
        //EventUtil.addEvent(window, "load", medal);
        $(function () { medal(); })
    
</script>

