        function ReturnToTop() {
            var isIE6 = !!(window.attachEvent && !window.opera && navigator.userAgent.indexOf("MSIE 6") > -1);
            var scrollTop = Math.max(document.documentElement.scrollTop, document.body.scrollTop);
            var obj = document.getElementById("returnTop");
            if (scrollTop > 0) {
                if (obj.style.display == "none") {
                    obj.style.display = "";
                }
                if (isIE6) {
                    obj.style.top = scrollTop + document.documentElement.clientHeight - obj.offsetHeight - 40 + 'px';
                }
            }
            else {
                if (obj.style.display == "")
                    obj.style.display = "none";
            }
        }
        function ationcar(hisid) {
            if (ajaxCheckLogin()) {
                window.location.href = GetHost() + '/Garage/HerGarage?herUserId=' + hisid;
            }
        }
        //关注感兴趣的人     
        function AddInsetedFollow(userId,self) {
            if (ajaxCheckLogin()) {
                var clickFun = $(self).attr("onclick");
                $(self).removeAttr("onclick");
                $.ajax({
                    type: 'POST',
                    url: addfollwerUrl,
                    data: 'fuid=' + userId,
                    dataType: 'json',
                    success: function (data) {
                        if (data.Login == true) {
                            if (data.Result > -1) {
                                $("#a" + userId).hide();
                                $("#sp" + userId).show();
                                $("#a" + userId).parent().remove();
                            } else {   
                                $(self).attr("onclick",clickFun);
                                new PopWindow({ title: "提示", content: data.ErrorMessage });
                                return;
                            }

                        } else {                           
                            ajaxCheckLogin();
                            return;
                        }
                    }
                });
            }
        }
        $(function(){
            $.ajax({
                type: 'get',
                url: lastNoticeUrl,                   
                dataType: 'json',
                success: function (data) {
                    if(data!=null)
                    {
                        if (data.Login == true) {
                            if (data.Result.length>0) {
                                $("#divContent").html(data.Result);
                                $("#divNewNotice").show();
                            } else {   
                                $("#divNewNotice").hide();
                            }

                        } else {                           
                            ajaxCheckLogin();
                            return;
                        }
                    }}
            });
        });

