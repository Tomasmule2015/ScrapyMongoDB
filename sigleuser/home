    <script type="text/javascript">
        seajs.use(["otheruserinfo", "tcar", "tkbei", "TMyTopic", "jquery", "sorttable", "tab", "focus", "select", "layer", "tip", "operaterelations", "garagebox", "gotop"], function (otheruserinfo, tcar, tkbei, TMyTopic, $) {
            otheruserinfo.main();
            tcar.main();
            TMyTopic.main();
            tkbei.main();
            //焦点图
            $('#focusimg02').focus({
                anim: true,
                interval: 3000000,
                pause: 'hover'
            });
           
           
            $.fn.scrollFixed = function () {

                var opt = {
                    top: 0,
                    distance: 0
                },
                elem = this, t;

                elem.css({ position: 'static', top: 0 });

                t = elem.offset().top;

                $.extend(opt, arguments[0] || {});

                function set() {
                    if ($(window).scrollTop() + opt.distance > t) {
                        elem.css({
                            top: opt.top + 'px',
                            position: 'fixed'
                        });
                    }
                    else {
                        elem.css({
                            top: 0,
                            position: 'static'
                        });
                    }
                }

                $(window).on('scroll', set);

                set();
            };
            $(function () {
